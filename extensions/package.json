        "rxjs": "^6.3.3",
        "typescript": "^3.2.4",
        "ava": "^1.1.0",
        "benchmark": "^2.1.4",
        "ts-node": "^8.0.2",
        "@types/benchmark": "^1.0.31"
    },
    "main": "lib/src/extensions.js",
